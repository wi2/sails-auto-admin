{
  "name": "sails-auto-admin",
  "version": "0.0.1",
  "description": "Automatic admin depends on model with reactjs and newforms",
  "main": "dist/api/hooks/admin/index.js",
  "scripts": {
    "test": "gulp && mocha --reporter spec --compilers js:babel/register",
    "prepublish": "gulp",
    "preinstall": "npm install --save react@0.13 react-router newforms newforms-bootstrap rc-pagination auto-admin",
    "postinstall": "npm i browserify --save-dev && npm run build-js",
    "build-js": "browserify -r react -r react-router -r newforms -r newforms-bootstrap -r rc-pagination -r auto-admin > assets/js/admin/dependencies/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wi2/sails-auto-admin.git"
  },
  "keywords": [
    "sails",
    "sails.js",
    "react",
    "newforms",
    "admin",
    "auto-admin",
    "automatic"
  ],
  "author": "Mike WI2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wi2/sails-auto-admin/issues"
  },
  "homepage": "https://github.com/wi2/sails-auto-admin",
  "devDependencies": {
    "babel": "^5.8.21",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "jshint": "^2.8.0",
    "lodash": "^3.10.1",
    "mocha": "^2.x.x",
    "passport-local": "^1.0.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "request": "^2.58.0",
    "sails": "github:balderdashy/sails",
    "sails-disk": "^0.10.8",
    "sails-memory": "^0.10.5",
    "supertest": "^0.15.0"
  },
  "dependencies": {
    "auto-admin": "^0.2.8",
    "marlinspike": "^0.12"
  },
  "engines": {
    "node": ">= 0.10",
    "npm": ">= 2.3"
  },
  "sails": {
    "isHook": true,
    "hookName": "auto-admin"
  },
  "directories": {
    "test": "test"
  }
}
